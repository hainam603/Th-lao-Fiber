  <download-excel \r\n            name=\"DANH SÁCH FIBER PHÁT TRIỂN MỚI\" \r\n            :fields=\"columns\"\r\n            :data=\"tableData\">\r\n                <el-button icon=\"el-icon-download\">Excel</el-button>\r\n            </download-excel>\r\n          </div>\r\n    </v-card-title>\r\n    <v-card-text>\r\n        <el-table\r\n          v-loading=\"loading\"\r\n          border\r\n          height=\"400\"\r\n          :data=\"posts\"\r\n          style=\"width: 100%\">\r\n          <el-table-column fixed type=\"index\" width=\"50\"></el-table-column>\r\n          <el-table-column prop=\"ma_tb\" label=\"ma_tb\" width=\"180\"></el-table-column>\r\n          <el-table-column prop=\"tb_cd_dongquang\" label=\"tb_cd_dongquang\" width=\"180\"></el-table-column>\r\n          <el-table-column prop=\"huy_truoc_30ngay\" label=\"huy_truoc_30ngay\" width=\"180\"></el-table-column>\r\n          <el-table-column prop=\"khongthuoc_khuvuc_pttb\" label=\"khongthuoc_khuvuc_pttb\" width=\"200\"></el-table-column>\r\n          <el-table-column prop=\"thang_hoamang\" label=\"thang_hoamang\" width=\"180\"></el-table-column>\r\n          <el-table-column prop=\"ngay_hoamang\" label=\"ngay_hoamang\" width=\"190\"></el-table-column>\r\n          <el-table-column prop=\"ngay_ins\" label=\"ngay_ins\" width=\"180\"></el-table-column>\r\n          <el-table-column prop=\"trangthai\" label=\"trangthai\" width=\"180\"></el-table-column>\r\n          <el-table-column prop=\"goi_tratruoc\" label=\"goi_tratruoc\" width=\"180\"></el-table-column>\r\n          <el-table-column prop=\"tocdo_id\" label=\"tocdo_id\" width=\"180\"></el-table-column>\r\n          <el-table-column prop=\"ma_NVKT_TT\" label=\"ma_NVKT_TT\" width=\"180\"></el-table-column>\r\n          <el-table-column prop=\"hdtb_id\" label=\"hdtb_id\" width=\"180\"></el-table-column>\r\n          <el-table-column prop=\"hdkh_id\" label=\"hdkh_id\" width=\"180\"></el-table-column>\r\n          <el-table-column prop=\"thuebao_id\" label=\"thuebao_id\" width=\"180\"></el-table-column>\r\n          <el-table-column prop=\"kieuld_id\" label=\"kieuld_id\" width=\"180\"></el-table-column>\r\n          <el-table-column prop=\"ma_gd\" label=\"ma_gd\" width=\"180\"></el-table-column>\r\n          <el-table-column prop=\"phongbh\" label=\"phongbh\" width=\"180\"></el-table-column>\r\n          <el-table-column prop=\"phongbh_id\" label=\"phongbh_id\" width=\"180\"></el-table-column>\r\n          <el-table-column prop=\"diachi_ld\" label=\"diachi_ld\" width=\"380\"></el-table-column>\r\n          <el-table-column prop=\"ngay_tt\" label=\"ngay_tt\" width=\"180\"></el-table-column>\r\n          <el-table-column prop=\"tb_dungthu\" label=\"tb_dungthu\" width=\"180\"></el-table-column>\r\n          <el-table-column prop=\"ngay_tao\" label=\"ngay_tao\" width=\"180\"></el-table-column>\r\n        </el-table>\r\n        <v-row align=\"center\" justify=\"center\" no-gutters>\r\n            <el-pagination\r\n                background\r\n                @current-change=\"displayedPosts\"\r\n                @size-change=\"handleSizeChange\"\r\n                layout=\"total, sizes, prev, pager, next\"\r\n                :page-sizes=\"[50, 100, 150]\"\r\n                :page-size=\"perPage\"\r\n                :current-page=\"page\"\t\r\n                :total=\"total\">\r\n            </el-pagination>\r\n        </v-row>\r\n        \r\n    </v-card-text>\r\n  </v-card>\r\n</template>\r\n\r\n<script>\r\nimport Factory from '@/server/factory/factory';\r\nimport common from '@/server/common/common';\r\nconst New = Factory.get('new');\r\nexport default {\r\n    data() {\r\n        return {\r\n\r\n            //Table New\r\n            loading:false,\r\n            tableData: [],\r\n            posts:[],\r\n            page: 1,\r\n            perPage: 50,\r\n            pages: [],\r\n            total:0,\r\n\r\n            //export excel\r\n            columns:\r\n            {\r\n                \"ma_tb\": \"ma_tb\",\r\n                \"tb_cd_dongquang\": \"tb_cd_dongquang\",\r\n                \"huy_truoc_30ngay\": \"huy_truoc_30ngay\",\r\n                \"khongthuoc_khuvuc_pttb\": \"khongthuoc_khuvuc_pttb\",\r\n                \"thang_hoamang\": \"thang_hoamang\",\r\n                \"ngay_hoamang\": \"ngay_hoamang\",\r\n                \"ngay_ins\": \"ngay_ins\",\r\n                \"trangthai\": \"trangthai\",\r\n                \"goi_tratruoc\": \"goi_tratruoc\",\r\n                \"tocdo_id\": \"tocdo_id\",\r\n                \"ma_NVKT_TT\": \"ma_NVKT_TT\",\r\n                \"hdtb_id\": \"hdtb_id\",\r\n                \"hdkh_id\": \"hdkh_id\",\r\n                \"thuebao_id\": \"thuebao_id\",\r\n                \"kieuld_id\": \"kieuld_id\",\r\n                \"ma_gd\": \"ma_gd\",\r\n                \"phongbh\": \"phongbh\",\r\n                \"phongbh_id\": \"phongbh_id\",\r\n                \"ten_tb\": \"ten_tb\",\r\n                \"diachi_ld\": \"diachi_ld\",\r\n                \"ngay_tt\": \"ngay_tt\",\r\n                \"tb_dungthu\": \"tb_dungthu\",\r\n                \"ngay_tao\": \"ngay_tao\"\r\n            },\r\n            //userlogin\r\n            ma_ND: null,\r\n            ten_NV: null,\r\n            token: null,\r\n        }\r\n    },\r\n    created () {\r\n        var self=this;\r\n        //Get Info User Login\r\n        var UserLogin = common.GetInfoUserLogin('token_session');\r\n        if(!UserLogin)\r\n            self.$router.push('/login');\r\n        else\r\n        {\r\n            self.ten_NV=UserLogin.ten_NV;\r\n            self.ma_ND=UserLogin.ma_ND;\r\n            self.token=UserLogin.token;\r\n        \r\n        };\r\n\r\n        //Get list New\r\n\r\n        self.Get_List_New();\r\n    },\r\n    methods: {\r\n        Get_List_New() {\r\n            var self=this;\r\n            self.loading=true;\r\n            New.Get_List_New('',self.token).then(response =>{\r\n                if(response.data.success && response.data.data.length){\r\n                    self.tableData=response.data.data\r\n                    self.total=self.tableData.length\r\n                    self.posts=self.paginate(self.tableData)\r\n                    self.loading=false\r\n                }\r\n\r\n            });\r\n            \r\n        },\r\n        //phân trang\r\n        displayedPosts(e){\r\n                var self=this;\r\n                self.loading=true;\r\n                self.page=e;\r\n                self.posts=self.paginate(self.tableData);\r\n                self.loading=false;\r\n        },\r\n        paginate (posts) {\r\n                var self=this;\r\n                var page = self.page;\r\n                var perPage = self.perPage;\r\n                var from = (page * perPage) - perPage;\r\n                var to = (page * perPage);\r\n                return  posts.slice(from, to);\r\n        },\r\n        handleSizeChange(e){\r\n            var self=this;\r\n            self.perPage=e;\r\n            self.displayedPosts(1);\r\n        },\r\n        //\r\n        exportToExcel(){\r\n            var data=this.tableData;\r\n                json2excel({\r\n                    data,\r\n                    name: 'user-info-data',\r\n                    formateDate: 'yyyy/mm/dd'\r\n                });\r\n    \r\n        },\r\n    },\r\n}\r\n</script>\r\n\r\n<style>\r\n\r\n</style>"]}]}