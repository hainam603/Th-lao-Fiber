;AA9BA,KAAA;AAAA,GAPA;AA0CA,EAAA,OA1CA,qBA0CA;AACA,QAAA,IAAA,GAAA,IAAA,CADA,CAEA;;AACA,IAAA,IAAA,CAAA,SAAA,CAAA,OAAA,CAAA,UAAA,IAAA,EAAA;AACA,MAAA,IAAA,CAAA,WAAA,CAAA,IAAA,CAAA,IAAA,CAAA,KAAA;AACA,KAFA,EAHA,CAOA;;AACA,QAAA,SAAA,GAAA,MAAA,CAAA,gBAAA,CAAA,eAAA,CAAA;AACA,QAAA,CAAA,SAAA,EACA,IAAA,CAAA,OAAA,CAAA,IAAA,CAAA,QAAA,EADA,KAGA;AACA,MAAA,IAAA,CAAA,MAAA,GAAA,SAAA,CAAA,MAAA;AACA,MAAA,IAAA,CAAA,KAAA,GAAA,SAAA,CAAA,KAAA;AACA,MAAA,IAAA,CAAA,KAAA,GAAA,SAAA,CAAA,KAAA;AAEA;AAEA,GA7DA;AA8DA,EAAA,KAAA,EAAA;AACA,IAAA,WAAA,EAAA,qBAAA,GAAA,EAAA;AACA,UAAA,IAAA,GAAA,IAAA;AACA,MAAA,IAAA,CAAA,aAAA,GAAA,IAAA;;AACA,UAAA,IAAA,CAAA,WAAA,CAAA,QAAA,CAAA,GAAA,CAAA,EACA;AACA,QAAA,IAAA,CAAA,SAAA,CAAA,OAAA,CAAA,UAAA,IAAA,EAAA;AACA,cAAA,IAAA,CAAA,KAAA,IAAA,GAAA,EAAA;AACA,YAAA,IAAA,CAAA,OAAA,CAAA,IAAA,CAAA,IAAA,CAAA,IAAA;AACA;AACA,SAJA;AAKA;;AACA,MAAA,IAAA,CAAA,aAAA,GAAA,KAAA;AACA;AAbA,GA9DA;AA6EA,EAAA,OAAA,EAAA;AACA;AACA,IAAA,iBAFA,6BAEA,GAFA,EAEA;AACA,UAAA,GAAA,IAAA,UAAA,EACA,KAAA,OAAA,CAAA,IAAA,CAAA,YAAA,EADA,KAGA,KAAA,OAAA,CAAA,IAAA,CAAA,SAAA;AACA,KAPA,CAQA;;AARA;AA7EA,CAAA","sourcesContent":["<template>\n  <v-app >\n\n    <!-- menu -->\n     \n     <v-app-bar app color=\"blue darken-1\" dark v-if=\"$route.meta.requiresAuth\">\n        \n        <v-toolbar-Title>\n          <v-img\n            alt=\"VNPT Logo\"\n            class=\"shrink mr-2\"\n            contain\n            src=\"../public/logo-vnpt.png\"\n            transition=\"scale-transition\"\n            width=\"70\"\n          />\n        </v-toolbar-Title>\n        <v-autocomplete\n        v-model=\"searchSelect\"\n        :loading=\"searchLoading\"\n        :items=\"searchItems\"\n        :search-input.sync=\"searchInput\"\n        cache-items\n        class=\"mx-4\"\n        flat\n        hide-no-data\n        hide-details\n        label=\"Tìm kiếm\"\n        solo-inverted\n      ></v-autocomplete>\n        <v-spacer></v-spacer>\n        <v-toolbar-items\n          class=\"hidden-xs-only\"\n          v-for=\"(item,i) in menuItems\"\n          :key=\"i\"\n        >\n          <v-btn text :to=\"item.path\">\n            <v-icon left dark>{{ item.icon }}</v-icon>\n            <span class=\"mr-2\">{{ item.title }}</span>\n          </v-btn>\n\n          <v-divider vertical></v-divider>\n        </v-toolbar-items>\n        <v-app-bar-nav-icon\n          @click.stop=\"sidebar=!sidebar\"\n        ></v-app-bar-nav-icon>\n      </v-app-bar>\n\n      <!-- sidebar left -->\n\n      <v-navigation-drawer v-model=\"sidebar\" temporary absolute bottom right>\n          <v-list shaped>\n\n            <v-list-group :value=\"true\" prepend-icon=\"mdi-account-circle\">\n              <template v-slot:activator>\n                <v-list-item-Title>Người dùng</v-list-item-Title>\n              </template>\n\n              <v-list-group :value=\"true\" no-action sub-group>\n                <template v-slot:activator>\n                  <v-list-item-content>\n                    <v-list-item-Title>Chức năng</v-list-item-Title>\n                  </v-list-item-content>\n                </template>\n\n                <v-list-item v-for=\"(item,index) in functionsUser\" :key=\"index\" @click=\"clickFunctionUser(item.name)\" link>\n                  <v-list-item-icon>\n                    <v-icon>{{item.icon}}</v-icon>\n                  </v-list-item-icon>\n                  <v-list-item-Title>{{item.name}}</v-list-item-Title>\n                </v-list-item>\n              </v-list-group>\n            </v-list-group>\n          </v-list>\n        </v-navigation-drawer>\n\n    <!-- main -->\n\n    <v-main>\n       <router-view/>\n    </v-main>\n\n    \n\n    <!-- menu bottom -->\n    <!-- <v-bottom-navigation\n      v-model=\"valueMenuBottom\"\n      :input-value=\"activeMenuBottom\"\n      color=\"white\"\n      background-color=\"blue darken-1\"\n      dark\n    >\n      <v-btn>\n        <span>Recents</span>\n\n        <v-icon>mdi-history</v-icon>\n      </v-btn>\n\n      <v-btn>\n        <span>Favorites</span>\n\n        <v-icon>mdi-heart</v-icon>\n      </v-btn>\n\n      <v-btn>\n        <span>Nearby</span>\n\n        <v-icon>mdi-map-marker</v-icon>\n      </v-btn>\n    </v-bottom-navigation> -->\n  \n  </v-app>\n</template>\n\n<script>\n// import HelloWorld from './components/HelloWorld';\nimport common from '@/server/common/common';\n\nexport default {\n  name: 'App',\n\n  components: {\n    // HelloWorld,\n  },\n\n  data: () => ({\n    //menu\n      \n      menuItems: [\n          { title: 'Trang chủ', path: '/home', icon: 'mdi-home' },\n          { title: 'Danh sách phát triển mới', path: '/new', icon: 'mdi-open-in-new' },\n          { title: 'Tính thù lao lần 1', path: '/fee1', icon: 'mdi-account-network' },\n          { title: 'Tính thù lao lần 2', path: '/fee2', icon: 'mdi-account-network-outline' }\n      ],\n      //sidebar menu\n      sidebar: false,\n      functionsUser:[\n        {name:\"Quản trị\", icon:\"mdi-account-group\"},\n        {name:\"Đăng xuất\", icon:\"mdi-logout\"},\n      ],\n\n\n      //searchBox\n      searchItems: [],\n      searchInput:null,\n      searchLoading:false,\n      searchSelect:null,\n      \n      //menuBottom\n      valueMenuBottom:null,\n      activeMenuBottom:null,\n\n      //userlogin\n      ma_ND: null,\n      ten_NV: null,\n      token: null,\n\n\n  }),\n\n  created(){\n    var self=this;\n    //Load List Items Searchbox\n    self.menuItems.forEach(item=>{\n      self.searchItems.push(item.title);\n    });\n\n    //Get Info User Login\n    var UserLogin = common.GetInfoUserLogin('token_session');\n    if(!UserLogin)\n      self.$router.push('/login');\n    else\n    {\n      self.ten_NV=UserLogin.ten_NV;\n      self.ma_ND=UserLogin.ma_ND;\n      self.token=UserLogin.token;\n      \n    }\n\n  },\n  watch:{\n    searchInput: function(val){\n      var self=this;\n      self.searchLoading=true;\n      if(self.searchItems.includes(val))\n      {\n        self.menuItems.forEach(item=>{\n          if(item.title==val){\n             self.$router.push(item.path);\n          }\n        });\n      }\n      self.searchLoading=false;\n    }\n  },\n  methods:{\n    //sidebar\n    clickFunctionUser(val){\n      if(val==\"Quản trị\")\n        this.$router.push('/adminpage');\n      else\n        this.$router.push('/logout');\n    },\n    //get info user login\n    \n  }\n};\n</script>\n<style scoped>\n\n</style>\n"],"sourceRoot":"src"}]}